# all:
	# g++ test.cpp Square.cpp -o test
#compiler type set to g++
CC=g++
#flagsh for c++ compiler
CFLAGS=-c -g -Wall

all: test

test: test.o Square.o
	$(CC) test.o Square.o -o test

test.o: test.cpp
	$(CC) $(CFLAGS) test.cpp

Square.o: Square.cpp
	$(CC) $(CFLAGS) Square.cpp

#Queue.o: Queue.cpp
#	$(CC) $(CFLAGS) Queue.cpp
clean:
	rm -rf *o test